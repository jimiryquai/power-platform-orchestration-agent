{
  "name": "power-platform-orchestration-agent",
  "version": "1.0.0",
  "description": "Intelligent orchestration agent using n8n that automates enterprise-level Power Platform project setup and management",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "setup:n8n": "docker-compose -f docker/docker-compose.yml up -d",
    "setup:dev": "npm run setup:n8n && npm run setup:templates",
    "setup:templates": "node scripts/setup-templates.js"
  },
  "keywords": [
    "power-platform",
    "azure-devops",
    "n8n",
    "orchestration",
    "automation",
    "mcp"
  ],
  "author": "Enterprise Orchestration Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "redis": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}